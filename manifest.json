{
    "name": "Slasher",
    "description": "Will replace slash with a picture of slash",
    "version": "1.0",
    "manifest_version": 2,

    "content_scripts": [ {
        "js": [ "jquery.js", "background.js" ],
        "matches": [ "http://*/*", "https://*/*"]
    }],

    "web_accessible_resources": [
    "man.jpg"
    ]
}